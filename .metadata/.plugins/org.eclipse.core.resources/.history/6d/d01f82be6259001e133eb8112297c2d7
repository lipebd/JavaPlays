import java.util.Scanner;

public class fatorar {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		System.out.print("Número a fatorar: ");
		int n = sc.nextInt();
		int resultado = n
		while (n > 1) {
			resultado *= (n-1);
			
		}
		
		System.out.println("O resultado é: " + resultado);
		
		sc.close();
	}
}